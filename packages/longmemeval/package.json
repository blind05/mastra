{
  "name": "@mastra/longmemeval",
  "version": "0.1.0",
  "description": "LongMemEval benchmark implementation for Mastra Memory",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "generate-embeddings": "tsx scripts/generate-embeddings.ts",
    "setup": "tsx scripts/setup.ts",
    "download": "tsx scripts/download.ts",
    "cli": "tsx src/cli.ts",
    "prepare:s": "tsx src/cli.ts prepare -d longmemeval_s -c semantic-recall",
    "bench:s": "tsx src/cli.ts run --dataset longmemeval_s --model gpt-4o --memory-config semantic-recall --concurrency 10",
    "prepare:s:working": "tsx src/cli.ts prepare -d longmemeval_s -c working-memory",
    "bench:s:working": "tsx src/cli.ts run --dataset longmemeval_s --model gpt-4o --memory-config working-memory --concurrency 10",
    "prepare:s:lastk": "tsx src/cli.ts prepare -d longmemeval_s -c last-k",
    "bench:s:lastk": "tsx src/cli.ts run -d longmemeval_s -m gpt-4o -c last-k",
    "prepare:s:combined": "tsx src/cli.ts prepare -d longmemeval_s -c combined",
    "bench:s:combined": "tsx src/cli.ts run -d longmemeval_s -m gpt-4o -c combined"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/provider": "^1.1.3",
    "@ai-sdk/provider-utils": "^2.2.8",
    "@huggingface/hub": "^0.15.1",
    "@mastra/core": "workspace:*",
    "@mastra/fastembed": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "@mastra/memory": "workspace:*",
    "ai": "^3.4.33",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "imvectordb": "^0.0.6",
    "openai": "^4.73.1",
    "ora": "^8.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20"
  }
}
