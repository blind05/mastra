{
  "name": "@mastra/longmemeval",
  "version": "0.1.0",
  "description": "LongMemEval benchmark implementation for Mastra Memory",
  "scripts": {
    "setup": "tsx scripts/setup.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "cli": "tsx src/cli.ts",
    "download": "tsx scripts/download.ts",
    "bench:s": "tsx src/cli.ts run --dataset longmemeval_s --model gpt-4o",
    "bench:m": "tsx src/cli.ts run --dataset longmemeval_m --model gpt-4o",
    "bench:oracle": "tsx src/cli.ts run --dataset longmemeval_oracle --model gpt-4o",
    "bench:s:quick": "tsx src/cli.ts run --dataset longmemeval_s --model gpt-4o --subset 10",
    "bench:m:quick": "tsx src/cli.ts run --dataset longmemeval_m --model gpt-4o --subset 10",
    "bench:oracle:quick": "tsx src/cli.ts run --dataset longmemeval_oracle --model gpt-4o --subset 10"
  },
  "dependencies": {
    "@mastra/core": "workspace:*",
    "@mastra/memory": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "@ai-sdk/openai": "^1.0.0",
    "@huggingface/hub": "^0.15.1",
    "ai": "^3.4.33",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "openai": "^4.73.1",
    "ora": "^8.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20"
  }
}