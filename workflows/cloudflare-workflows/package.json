{
  "name": "@mastra/cloudflare-workflows",
  "version": "0.11.0",
  "description": "Mastra Cloudflare Workflows integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./engine": {
      "import": "./dist/engine.js",
      "require": "./dist/engine.cjs"
    },
    "./workers": {
      "import": "./dist/workers.js",
      "require": "./dist/workers.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "wrangler types && tsup",
    "build:watch": "pnpm build --watch",
    "test": "vitest run --no-isolate --bail=1 --retry=1",
    "lint": "eslint .",
    "wrangler:types": "wrangler types"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@mastra/core": "workspace:*",
    "@mastra/deployer": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "@types/node": "^20.19.0",
    "eslint": "^9.30.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.1"
  },
  "peerDependencies": {
    "@mastra/core": ">=0.11.0-0 <0.12.0-0"
  }
}
