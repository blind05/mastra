{
  "name": "@mastra/prisma",
  "version": "0.0.1",
  "description": "Prisma ORM provider for Mastra - provides storage capabilities with type-safe database access",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:watch": "pnpm build --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "pretest": "docker compose up -d && pnpm prisma:generate && pnpm prisma:migrate",
    "test": "vitest run",
    "posttest": "docker compose down -v",
    "test:watch": "vitest watch",
    "lint": "eslint ."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@prisma/client": "^6.0.0"
  },
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@internal/storage-test-utils": "workspace:*",
    "@mastra/core": "workspace:*",
    "@microsoft/api-extractor": "^7.52.8",
    "@types/node": "^20.19.0",
    "eslint": "^9.35.0",
    "prisma": "^6.12.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@internal/types-builder": "workspace:*",
    "dotenv": "^16.4.5"
  },
  "peerDependencies": {
    "@mastra/core": ">=0.17.0-0 <0.19.0-0"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "stores/prisma"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  }
}